<table class="ui table">
  <thead>
    <tr>
        <th>Timestamp</th>
        <th>Code</th>
        <th>Temperature</th>
        <th>WindSpeed</th>
        <th>Wind Direction</th>
        <th>Pressure</th>
        <th></th>
    </tr>
  </thead>
  <tbody>
        {{#each station.readings}}
        <tr>
          <td>
           {{date}}
        </td>
            <td>
              {{code}}
            </td>
          <td>
              {{temperature}}
            </td>
            <td>
              {{windSpeed}}
            </td>
          <td>
              {{windDirection}}
          </td>
          <td>
              {{pressure}}
          </td>
          <td>
          <a href="/station/{{../station.id}}/deletereading/{{id}}" class="ui icon button">
            <i class="icon trash"></i></a>
          </td>
          </tr>
        {{/each}}
      </tbody>
</table>