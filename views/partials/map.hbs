<style>#map { height: 250px; }</style>

<div id="map"></div>

<script
  src="https://unpkg.com/leaflet@1.8.0/dist/leaflet.js"
  integrity="sha512-BB3hKbKWOc9Ez/TAwyWxNXeoV9c1v6FIeYiBieIWkpLjauysF18NzgR1MBNBXf8/KABdlkX68nAhlwcDFLGPCQ=="
  crossorigin=""
></script>

<script>
  var map = L.map('map').setView([53.350, -6.2609], 7);
  map.scrollWheelZoom.disable();
  L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', { maxZoom: 19,
  attribution: 'Â© OpenStreetMap' }).addTo(map); const stationCollection = [
  {{#each stations}}
    { id: '{{id}}', name: '{{name}}', lat:
    {{latitude}}, lon:
    {{longitude}}, },
  {{/each}}
  ]; for (const station of stationCollection) { var marker =
  L.marker([station.lat, station.lon]).addTo(map);
  marker.bindPopup(station.name).openPopup(); }

</script>